import java.util.*;

public class HSet
{

    public static void main(String args[])
    {

	// Definir 3 instancias de la Clase Persona
        Producto a = new Producto("Jon", "Auzmendi", 21);
        Producto b = new Producto("Telmo", "Arzelus", 21);
       Producto c = new Producto("Odei", "Alba", 20);

	// Definir un HashSet
        HashSet stock = new HashSet();
        stock.add(d);
        stock.add(j);
        stock.add(cd);

	// Imprimir contenido de HashSet

        System.out.println(" - Lista de pedido con " + stock.size() + " elementos");

	// Definir Iterator para extraer/imprimir valores 

        for( Iterator it = stock.iterator(); it.hasNext();) { 
	    Producto x = (Producto)it.next();
	    System.out.println(x.nombre + " : " + x.cantidad);

	}

	// Eliminar todos los valores del ArrayList
	stock.clear();
        System.out.println(" - Lista de pedido final con " + pedido.size() + " elementos");
    }
}
